<form>
  </div>
  <div class="mb-3">
    <label for="registerUsername" class="form-label">Username:</label>
    <input type="text" autocomplete="username" class="form-control" id="registerUsername">
  </div>
  <div class="mb-3">
    <label for="registerEmail" class="form-label">Email address</label>
    <input type="email" autocomplete="email" class="form-control" id="registerEmail" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  <div class="mb-3">
    <label for="registerPass" class="form-label">Password:</label>
    <input type="password" autocomplete="new-password" class="form-control" id="registerPass">
  </div>
  <div class="mb-3">
    <label for="registerPassConfirm" class="form-label">Confirm Password:</label>
    <input type="password" autocomplete="new-password" class="form-control" id="registerPassConfirm">
  </div>
  <div>
    <p id="registerResult"></p>
  </div>
</form>
