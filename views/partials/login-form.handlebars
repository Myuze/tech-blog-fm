<form>
  <div class="mb-3">
    <label for="loginEmail" class="form-label">Email address</label>
    <input type="email" autocomplete="email" class="form-control" id="loginEmail" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  </div>
  <div class="mb-3">
    <label for="inputPass" class="form-label">Password</label>
    <input type="password" autocomplete="current-password" class="form-control" id="inputPass">
  </div>
  <div>
      <button id="signUpBtn" class="btn btn-primary">Don't have a login? Sign up Here!</button>
  </div>
  <div>
    <p id="modalResult"></p>
  </div>
</form>
